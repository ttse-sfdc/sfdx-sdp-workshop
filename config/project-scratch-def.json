{
    "orgName": "Salesforce Data Pipelines Workshop SFDX",
    "description": "Scratch org config for Data Pipelines Workshop",
    "edition" : "Developer",
    "country": "US",
    "adminEmail": "ttse@salesforce.com",
    "features" : [
        "SalesUser",
        "ServiceUser",
        "ServiceCloud",
        "MarketingUser",
        "DataProcessingEngine",
        "EnableSetPasswordInApi"
    ],
    "settings": {
          "accountSettings": {
            "enableRelateContactToMultipleAccounts": true
          },
          "lightningExperienceSettings": {
              "enableS1DesktopEnabled": true
          },
          "securitySettings": {
            "enableAuditFieldsInactiveOwner": true
          },
          "analyticsSettings": {
            "sonicEnabled":true,
            "enableSnowflakeOutputConnector":true,
            "enableSalesforceOutputConnector": true,
            "enableTableauHyperOutputConnector": true
          }
      }
  }
